{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"types": [
			"vite/client"
		],
		"composite": true,
		"useDefineForClassFields": true,
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext"
		],
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "Node",
		"jsx": "react-jsx"
	},
	"include": [
		"src"
	]
}
